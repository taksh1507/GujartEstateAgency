{
  "name": "gujarat-real-estate",
  "version": "2.1.0",
  "description": "A comprehensive real estate management platform with Firebase and Cloudinary integration",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd admin-dashboard && npm run dev",
    "build": "cd admin-dashboard && npm run build",
    "start": "cd backend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../admin-dashboard && npm install"
  },
  "keywords": [
    "real-estate",
    "property-management",
    "react",
    "nodejs",
    "firebase",
    "cloudinary",
    "admin-dashboard"
  ],
  "author": "Gujarat Real Estate Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taksh1507/GujaratRealEstate.git"
  },
  "bugs": {
    "url": "https://github.com/taksh1507/GujaratRealEstate/issues"
  },
  "homepage": "https://github.com/taksh1507/GujaratRealEstate#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}